{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Debug (OpenOCD)",
          "cwd": "${workspaceRoot}",
          "executable": "${workspaceRoot}/build/test_osc.elf",
          "request": "attach",
          "type": "cortex-debug",
          "servertype": "openocd",
          "interface": "swd",
          "device": "STM32H725IETx",
          "svdFile": "${workspaceRoot}/svd/STM32H725.svd",
          "configFiles": [
              "${workspaceRoot}/nts-1_mkii.cfg"
          ],
          "postAttachCommands": [
            "dir ${workspaceRoot}/test-osc",
            "add-symbol-file ${workspaceRoot}/build/test_osc.elf 0x14aa0",
            "c"
          ]
      }
  ]
}
  